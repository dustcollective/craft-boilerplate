# Ignore everthing in craft...
craft/*

# except plugins...
!craft/plugins/

# ...translations...
!craft/translations/

# ...the custom admin logo...
!craft/storage/logo/

# ...non-sensitive configuration...
!craft/config/
craft/config/db.php
craft/config/license.key

# ...and your templates...
!/craft/templates/

# ...except templates generated from src.
/craft/templates/_partials/favicons.html
/craft/templates/_partials/inlinejs/

# Now ignore everything in public...
public/*

# ...except assets...
!public/assets/

# ...but do things generated from src.
public/assets/css/
public/assets/js/
public/assets/img/

# Ignore dependencies.
bower_components/
node_modules/

# Ignore debris.
.sass-cache/
*.css.map
.DS_Store

# Ignore local development stuff.
nginx.conf
log/
