# Ignore everthing in craft...
craft/*

# ...except plugins...
!craft/plugins/

# ...translations...
!craft/translations/

# ...the custom admin logo...
!craft/storage/logo/

# ...non-sensitive configuration...
!craft/config/
craft/config/db.php
craft/config/license.key

# ...and your templates...
!/craft/templates/

# ...except templates generated from src.
/craft/templates/_partials/favicons.html
/craft/templates/_partials/inline/

# Now ignore everything in public...
public/*

# ...except locales...
#!public/en/

# ...and assets...
!public/assets/

# ...but do ignore assets generated from src.
public/assets/css/
public/assets/js/
public/assets/img/

# Now ignore dependencies...
bower_components/
node_modules/

# ...debris...
.sass-cache/
*.css.map
.DS_Store

# ...and local development stuff.
nginx.conf
log/
